{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "durable-functions-poc",
      "storageProvider": {
        "type": "Netherite",
        "StorageConnectionName": "AzureWebJobsStorage",
        "EventHubsConnectionName": "EventHubsConnection"
      }
    }
  },
  "logging": {
    "logLevel": {
      "DurableFunctionPoC": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }

}